<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.suts.mytestapplication1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainactiviy_mvp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".event_dispatch.DispatchTouchEventActivity" />
        <activity android:name=".netTest.NetTestActivity" />
        <activity android:name=".SectionAdapterActivity" />
        <activity android:name=".MultiItemAdapterActivity" />

        <service
            android:name=".aidltest.MyAidlTestService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.suts.mytestapplication1.AIDL_TEST_ACTION" />
            </intent-filter>
        </service>

        <activity android:name=".aidltest.AidlTestActivity" />
        <activity android:name=".mvp.MVPTestActivity" />
        <activity android:name=".myviews.MyPullToRefreshViewActivity" />
        <activity android:name=".AndroidDesignViewTest" />
        <activity android:name=".Game2048Activity"></activity>
    </application>

</manifest>